<Window x:Class="BinaryTreeCollection.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Коллекция Бинарного Дерева" Height="450" Width="800">
    <Grid>
        <ListBox x:Name="listBoxItems" HorizontalAlignment="Left" Height="249" Margin="10,0,0,0" VerticalAlignment="Top" Width="200"/>
        <Button Content="Добавить случайный элемент" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="229,20,0,0" Click="AddRandomNumber_Click"/>
        <Button Content="Удалить выбранный элемент" HorizontalAlignment="Left" VerticalAlignment="Top" Width="200" Margin="229,72,0,0" Click="RemoveSelectedItem_Click"/>
    </Grid>
</Window>
